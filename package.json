{
  "name": "news-app-react",
  "version": "0.0.1",
  "description": "News Feed using NewsAPI.org API",
  "main": "src/Index.js",
  "keywords": [
    "News",
    "NewsAPI.org",
    "PWA"
  ],
  "author": "Prasanta Saha",
  "license": "ISC",
  "private": true,
  "dependencies": {
    "@fortawesome/fontawesome": "^1.1.4",
    "@fortawesome/fontawesome-free-solid": "^5.0.8",
    "bulma": "^0.6.2",
    "immutable": "^3.8.2",
    "moment": "^2.20.1",
    "newsapi": "^2.2.0",
    "prop-types": "^15.6.1",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-fontawesome": "^1.6.1",
    "react-redux": "^5.0.7",
    "react-scripts": "^2.1.3",
    "react-swipe-events": "^1.0.2",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "create-react-app": "^2.1.3",
    "eslint": "^5.6.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "node-sass-chokidar": "^0.0.3",
    "node-sass-import": "^1.1.1"
  },
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "firebase deploy",
    "build-css": "node-sass-chokidar src/ -o src/ --importer node_modules/node-sass-import",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive --importer node_modules/node-sass-import",
    "start": "react-scripts start",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}
